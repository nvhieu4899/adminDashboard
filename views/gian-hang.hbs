<title>{{{title}}}</title>
<div class="col-lg-12 col-sm-6">
    <div class="card">
        <div class="card-header">
            <strong class="card-title">Quản lí gian hàng</strong>
        </div>
        <div class="card-body">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th scope="col">Tên gian hàng</th>
                        <th scope="col">Số mặt hàng</th>
                        <th scope="col">Số lượt mua</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each cate}}
                    <tr>
                        <th scope="row">{{{name}}}</th>
                        <td>{{{sl}}}</td>
                        <td>{{{sold}}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>

        </div>
    </div>
</div>